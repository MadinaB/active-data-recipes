from: unittest
select:
    - {aggregate: count}
groupby:
    - {value: result.test}
    - {value: run.suite}
where:
    and:
        - eq: {result.status: SKIP}
        - gte: {run.stats.end_time: {date: {$eval: from_date}}}
        - lte: {run.stats.end_time: {date: {$eval: to_date}}}
limit: 1000
format: table